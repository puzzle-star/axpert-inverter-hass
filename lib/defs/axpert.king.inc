# Calculated

function axpert_calculate_power_factor {
	local ACTIVE_POWER="10#$1"
	local APPARENT_POWER="10#$2"
	local PF="100"

	[ "0" -ne "$[ $APPARENT_POWER ]" ] && PF="00$[ $[ $ACTIVE_POWER * 100 + $APPARENT_POWER / 2 ] / $APPARENT_POWER ]"

	PF="${PF: -3:3}"

	echo "${PF:0:1}.${PF:1:2}"
}


